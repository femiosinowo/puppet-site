output {
  
  
  rabbitmq {
  	  exchange => "exchangelogstash"
  	  exchange_type => "topic"
  	  key => "logs-%{host}"
  	  workers => 2
  	  durable => true
  	  persistent => true
  	  user	=> "logstash"
  	  password => "logstash"
  	  host => ["<%= @rabbitmq_service %>"]
  	  port	=> 5672
  	  vhost => "/logstash"
	}
	stdout { codec => rubydebug }
}